  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Timeless
  pod 'ReactiveKit'
  pod 'Bond'

  target 'TimelessTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'TimelessUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
